
public class PT1_1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// 單行註解
		/* 多
		 * 行
		 * 註
		 * 解
		 */
		String a = "XD";
		System.out.print(a+"This is first string");
		System.out.print("This is second string\n");
		// println以換行符號為結束符號
		System.out.println("This is third string");
		System.out.println("This is forth sting");
	}

}
